{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "index_fastapi.py",
      "use": "@vercel/python"
    },
    {
      "src": "index_flask.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "index.json" }
  ] 
}