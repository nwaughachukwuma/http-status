{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "index_fastapi.py",
      "use": "@vercel/python"
    },
    {
      "src": "index_flask.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    { "src": "/fastapi/(.*)", "dest": "index_fastapi.py" },
    { "src": "/flask/(.*)", "dest": "index_flask.py" },
    { "src": "/(.*)", "dest": "index.js" }
  ] 
}